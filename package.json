{
  "name": "q-extension-montastic",
  "displayName": "Create alerts from Montastic",
  "publisher": "Metadot Corporation",
  "version": "1.0.0",
  "description": "Checks the Montastic service for alerts, sends a signal if one is found.",
  "icon": "assets/icon.png",
  "main": "index.js",
  "author": "Cory Kim",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "watch": "nodemon ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corykim/q-extension-montastic.git"
  },
  "dependencies": {
    "daskeyboard-applet": "^2.2.8",
    "request-promise": "^4.2.2"
  },
  "qConfig": {
    "geometry": {
      "width": 1,
      "height": 1
    },
    "applet": {
      "defaults": {}
    },
    "requirements": {
      "authorization": "apiKey",
      "applet": {
        "monitors": [
          "string"
        ]
      }
    }
  }
}
